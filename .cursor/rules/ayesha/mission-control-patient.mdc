@router.get("/risk_assessment")
async def get_risk_assessment(patient_id: str):
    """
    Aggregate risk score from multiple sources.
    """
    # Get metastasis risk
    metastasis_risk = await metastasis_service.assess_risk(patient_id)
    
    # Get treatment failure probability
    treatment_risk = await treatment_service.predict_failure(patient_id)
    
    # Get resistance prediction
    resistance_risk = await sae_service.predict_resistance(patient_id)
    
    overall_risk = (
        metastasis_risk * 0.4 +
