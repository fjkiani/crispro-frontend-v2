# Oncology Frontend Environment Configuration
# Copy this to .env and fill in your actual values

# Backend API URLs
VITE_BACKEND_API_URL=https://your-backend-modal-url.modal.run
VITE_WEBSOCKET_URL=wss://your-backend-modal-url.modal.run/ws

# External Service URLs
VITE_FUSION_ENGINE_URL=https://crispro--fusion-engine-v1-fusionengine-api.modal.run
VITE_EVO_SERVICE_URL=your_evo_service_url_here
VITE_ZETA_ORACLE_URL=your_zeta_oracle_url_here

# API Keys (if needed on frontend)
VITE_GOOGLE_API_KEY=your_google_api_key_here
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration (for Analysis History)
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Feature Flags
VITE_ENABLE_AGENT_SYSTEM=true
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ANALYSIS_HISTORY=true

# Build Configuration
VITE_APP_NAME=Oncology CoPilot
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production 