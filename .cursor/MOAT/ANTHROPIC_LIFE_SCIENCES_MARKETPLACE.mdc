# single file: crispro-resistance-prediction/skill.py

from claude_skill import Skill, skill_tool

class CrisPROResistanceSkill(Skill):
    """Predict therapy resistance using ML models."""
    
    @skill_tool
    async def predict_resistance(
        self,
        mutations: list[str],  # e.g., ["KRAS:G12C", "TP53:R175H"]
        therapy: str,  # e.g., "osimertinib"
        cancer_type: str  # e.g., "NSCLC"
    ) -> dict:
        """Predict resistance probability and suggest validation experiments."""
        
        # Query SAE model (hosted on your infra or Replicate)
        resistance_score = await self._query_sae_model(mutations, therapy)
        
        # Query DepMap for synthetic lethal partners
        targets = await self._query_depmap(mutations, cancer_type)
        
        # Design sgRNAs for top targets
        sgrnas = await self._design_sgrnas(targets[:5])
        
        return {
            "resistance_probability": resistance_score,
            "mechanism": self._infer_mechanism(mutations),
            "validated_targets": targets,
            "suggested_sgrnas": sgrnas,
            "next_steps": self._generate_protocol(targets, sgrnas)
        }
```

